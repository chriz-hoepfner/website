    "@types/node": "20.11.30",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "autoprefixer": "10.4.19",
    "babel-loader": "9.1.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-plugin-storybook": "0.8.0",
    "flat": "6.0.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.13",
    "storybook-addon-turbo-build": "2.0.1",
    "storybook-tailwind-dark-mode": "1.0.22",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.3"
