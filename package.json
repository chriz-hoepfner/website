    "autoprefixer": "10.4.12",
    "babel-loader": "8.3.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-plugin-storybook": "0.6.8",
    "husky": "8.0.0",
    "postcss": "8.4.18",
    "prettier": "2.8.0",
    "prettier-plugin-tailwindcss": "0.2.0",
    "storybook-tailwind-dark-mode": "^1.0.15",
    "tailwindcss": "3.2.1",
    "typescript": "4.8.4"
  }
