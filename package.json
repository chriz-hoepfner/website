    "@types/node": "18.11.7",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.7",
    "autoprefixer": "10.4.17",
    "babel-loader": "8.3.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-plugin-storybook": "0.6.8",
    "flat": "5.0.2",
    "husky": "8.0.0",
    "jest": "29.3.1",
    "postcss": "8.4.18",
    "prettier": "2.8.0",
    "prettier-plugin-tailwindcss": "0.2.0",
    "storybook-addon-turbo-build": "1.1.0",
    "storybook-tailwind-dark-mode": "1.0.15",
    "tailwindcss": "3.2.1",
    "typescript": "4.8.4"
